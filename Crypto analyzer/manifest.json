{
    "name": "Cryto-Analyzer",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "120": "logo.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}